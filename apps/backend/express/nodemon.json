{
    "watch": [
        "src"
    ],
    "ext": "ts,js,json",
    "ignore": [
        "src/**/*.spec.ts",
        "src/**/*.test.ts",
        "dist"
    ],
    "exec": "ts-node --require tsconfig-paths/register src/index.ts",
    "env": {
        "NODE_ENV": "development"
    },
    "delay": "1000",
    "verbose": true
}